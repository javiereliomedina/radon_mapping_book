file = {:C$\backslash$:/Users/FU53VP/OneDrive - Aalborg Universitet/Dokumenter/FLOW/01 Biblio/Modelling/03{\_}Poot.pdf:pdf},
journal = {Investigaciones Regionales – Journal of Regional Research},
keywords = {Distance deterrence measurement,Gravity model,Migration flows,Parameter stability,Spatial interaction},
number = {36},
pages = {63--86},
title = {{The gravity model of migration: The successful comeback of an ageing superstar in regional science}},
