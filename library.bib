file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Minissale et al. - 1999 - The origin of natural gas emissions from Sardinia island, Italy.pdf:pdf},
issn = {00167002},
journal = {Geochemical Journal},
number = {1},
pages = {1--12},
title = {{The origin of natural gas emissions from Sardinia island, Italy}},
volume = {33},
year = {1999}
}
