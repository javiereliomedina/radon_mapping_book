isbn = {0272-7714},
issn = {02727714},
journal = {Estuarine, Coastal and Shelf Science},
keywords = {estuary,hypersalinity,normally closed,permanently open,river,seasonally open},
number = {3},
pages = {497--500},
publisher = {Elsevier Ltd},
title = {{The concept of an estuary: A definition that incorporates systems which can become closed to the ocean and hypersaline}},
url = {http://dx.doi.org/10.1016/j.ecss.2010.01.021},
volume = {87},
