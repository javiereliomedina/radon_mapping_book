isbn = {1750-5836},
issn = {17505836},
journal = {International Journal of Greenhouse Gas Control},
keywords = {CO 2,Geological storage,Health,Indoor exposure,Leakage,Unsaturated zone},
pages = {322--333},
pmid = {77339324},
publisher = {Elsevier Ltd},
title = {{Assessing health impacts of CO 2 leakage from a geological storage site into buildings: Role of attenuation in the unsaturated zone and building foundation}},
url = {http://dx.doi.org/10.1016/j.ijggc.2012.04.011},
volume = {9},
