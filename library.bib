isbn = {0377-0273},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {Calatrava Volcanic Province,Central Mexico,Central Spain,Michoac??n-Guanajuato Volcanic Field,Monogenetic volcanism,Volcanic alignments},
number = {1-4},
pages = {73--82},
publisher = {Elsevier B.V.},
