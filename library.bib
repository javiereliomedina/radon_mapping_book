doi = {10.3390/su11247098},
file = {:C$\backslash$:/Users/FU53VP/OneDrive - Aalborg Universitet/Dokumenter/Analysis/FLOW{\_}article{\_}02{\_}Housing{\_}prices/Biblio/Horak{\_}2019.pdf:pdf},
issn = {20711050},
journal = {Sustainability (Switzerland)},
keywords = {Accessibility,Gravity modelling,Journey to work,Public transport,Simulation,Trip distribution},
number = {24},
title = {{Modelling Public Transport Accessibility with Monte Carlo Stochastic Simulations: A Case Study of Ostrava}},
volume = {11},
year = {2019}
