journal = {Environmental Science and Technology},
number = {11},
pages = {2309--2313},
pmid = {11414037},
title = {{Field evaluation of a method for estimating gaseous fluxes from area sources using open-path fourier transform infrared}},
volume = {35},
year = {2001}
}
