file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Strand et al. - 2005 - High radon areas in Norway.pdf:pdf},
isbn = {0531-5131},
issn = {05315131},
journal = {International Congress Series},
keywords = {Radon,Radon concentrations,Radon surveys},
month = {feb},
