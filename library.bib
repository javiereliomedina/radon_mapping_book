@article{Laffineur2011a,
abstract = {We measured the isoprene and monoterpene fluxes continuously above a mixed forest site at Vielsalm in the eastern part of Belgium, using the disjunct eddy covariance technique with proton transfer reaction-mass spectrometry. Simultaneously, we also measured the carbon dioxide fluxes in order to deduce the gross primary production. The measurements were conducted from July to September 2009.During the day, the seasonal evolution of the isoprene/monoterpene emissions was studied using a monthly temperature and light dependence function deduced from our results to standardize the fluxes. A seasonal decrease in the standard emission factors was observed, probably linked to acclimation or senescence. The standard emission factor for isoprene fluxes (30 ??C, 1000 ??mol m-2 s-1) fell from 0.91 ?? 0.01 to 0.56 ?? 0.02 ??g m-2 s-1 and for monoterpene fluxes from 0.74 ?? 0.03 to 0.27 ?? 0.03 ??g m-2 s-1.During the night, a slight positive flux of monoterpenes was observed that seemed to be driven by air temperature. The standard emission factor (30??C) for nighttime monoterpene fluxes was equal to 0.093 ?? 0.019 ??g m-2 s-1.Finally, we studied the seasonal evolution of the relationship between the gross primary production and the isoprene/monoterpenes fluxes. A linear relationship was observed, highlighting the strong link between carbon assimilation and isoprene/monoterpene emissions. ?? 2011 Elsevier Ltd.},
author = {Laffineur, Q. and Aubinet, M. and Schoon, N. and Amelynck, C. and M{\"{u}}ller, J. F. and Dewulf, J. and {Van Langenhove}, H. and Steppe, K. and {\v{S}}impraga, M. and Heinesch, B.},
doi = {10.1016/j.atmosenv.2011.02.054},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Laffineur et al. - 2011 - Isoprene and monoterpene emissions from a mixed temperate forest.pdf:pdf},
isbn = {1352-2310},
issn = {13522310},
journal = {Atmospheric Environment},
keywords = {Disjunct eddy covariance,Forest ecosystems,Isoprene,Monoterpenes,PTR-MS},
number = {18},
pages = {3157--3168},
title = {{Isoprene and monoterpene emissions from a mixed temperate forest}},
volume = {45},
year = {2011}
}
@article{Imori2014a,
abstract = {In real-data analysis, deciding the best subset of variables in regression models is an important problem. Akaike's information criterion (AIC) is often used in order to select variables in many fields. When the sample size is not so large, the AIC has a non-negligible bias that will detrimentally affect variable selection. The present paper considers a bias correction of AIC for selecting variables in the generalized linear model (GLM). The GLM can express a number of statistical models by changing the distribution and the link function, such as the normal linear regression model, the logistic regression model, and the probit model, which are currently commonly used in a number of applied fields. In the present study, we obtain a simple expression for a bias-corrected AIC (corrected AIC, or CAIC) in GLMs. Furthermore, we provide an 'R' code based on our formula. A numerical study reveals that the CAIC has better performance than the AIC for variable selection. {\textcopyright} 2013 Board of the Foundation of the Scandinavian Journal of Statistics.},
author = {Imori, Shinpei and Yanagihara, Hirokazu and Wakaki, Hirofumi},
doi = {10.1111/sjos.12049},
file = {:C$\backslash$:/Users/FU53VP/OneDrive - Aalborg Universitet/Dokumenter/FLOW/01 Biblio/Modelling/online supporting information.pdf:pdf},
issn = {14679469},
journal = {Scandinavian Journal of Statistics},
keywords = {Information criterion,Maximum likelihood estimation,Stochastic expansion,Variable selection},
number = {2},
pages = {535--555},
title = {{Simple Formula for Calculating Bias-corrected AIC in Generalized Linear Models}},
volume = {41},
year = {2014}
}
