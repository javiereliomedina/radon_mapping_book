journal = {Energy Procedia},
keywords = {Fluid sampling,Monitoring,Nagaoka,Onshore aquifer,Seismic tomography,Simulation,Well logging},
number = {1},
pages = {2261--2268},
publisher = {Elsevier},
title = {{A monitoring framework for assessing underground migration and containment of carbon dioxide sequestered in an onshore aquifer}},
