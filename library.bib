doi = {10.1016/j.jssas.2016.05.003},
file = {:C$\backslash$:/Users/FU53VP/OneDrive - Aalborg Universitet/Dokumenter/FLOW/01 Biblio/Soil salinity/Asfaw 2018.pdf:pdf},
issn = {1658077X},
