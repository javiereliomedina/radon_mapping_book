issn = {17505836},
journal = {International Journal of Greenhouse Gas Control},
keywords = {Carbon storage,Monitoring,Otway project,Soil gas},
pages = {14--29},
publisher = {Elsevier Ltd},
title = {{Soil gas monitoring of the Otway Project demonstration site in SE Victoria, Australia}},
