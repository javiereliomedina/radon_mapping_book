issn = {09441344},
journal = {Environmental Science and Pollution Research},
keywords = {Accumulation chamber,CH4,CO2,Emission,Greenhouse gas,Landfill,Municipal solid waste},
number = {6},
pages = {1197--1206},
pmid = {20180032},
title = {{Gas emission into the atmosphere from controlled landfills: An example from Legoli landfill (Tuscany, Italy)}},
volume = {17},
year = {2010}
}
