@article{Schubert2001,
abstract = {One of the major problems facing risk assessment at polluted industrial sites and military bases is subsurface contamination by non-aq. phase-liqs. (NAPLs), since tracing the extent of a NAPL plume using conventional methods (drive point profiling) is usually assocd. with difficulties. In an effort to trace subsurface contamination as precisely as possible, monitoring points are placed in the area that might be affected by contaminants, and groundwater and soil samples are taken to the lab. for anal. However, the final no. of monitoring points is hardly ever sufficient for distinctive contamination mapping, and this may ultimately result in an unsuitable remediation action being taken. To obtain a more detailed image of a subsurface NAPL plume and, hence, to facilitate remediation measures that are best suited for the site in question, a denser network of monitoring points is desirable. The aim of the investigation described in this paper was therefore to develop a new detection method for subsurface NAPL contamination, which is based on an easily accessible indicator for NAPLs rather than on the anal. of soil and groundwater samples taken at the site. Based on the good soly. of radon in NAPLs, the idea was put forward that subsurface NAPL contamination should have an influence on the natural radon concn. of the soil gas. Provided this effect is significant, it would be possible to carry out a straightforward radon survey on an appropriate sampling grid covering the suspected site and thus enabling the NAPL contamination to be detected by the localization of anomalous low radon concns. in the soil. The overall aim of the investigation was to assess the general suitability of the soil-gas radon concn. as an indirect tracer for NAPL contamination in the ground. The partitioning coeff. KNAPL/air is one of the most influential parameters governing the decrease of the radon concn. in the soil gas in the presence of a subsurface NAPL contamination. Since NAPL mixts. such as gasoline, diesel fuel and paraffin are among the most important NAPLs regarding remediation activities, lab. expts. were performed to det. the radon-partitioning coeff. for these three NAPL mixts. Field expts. were carried out as well. For the field expts., each site was covered with a suitable grid of soil gas sampling points and the results were compared to the resp. findings of the earlier research performed by conventional means. The partitioning coeffs. achieved correspond to those published for pure NAPLs (Clever 1979) and are thus in the expected range. The results of the field expts. showed that the min. radon concns. detected match the resp. NAPL plumes traced previously. Both the results of the lab expts. and the on-site findings demonstrate that the soil-gas radon concn. can be used as an indicator for subsurface NAPL contamination. [on SciFinder (R)]},
author = {Schubert, Michael and Freyer, Klaus and Treutler, Hans-Christian and Wei{\ss}, Holger},
doi = {10.1007/BF02987728},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Schubert et al. - 2001 - Using the soil gas radon as an indicator for ground contamination by non-aqueous phase-liquids.pdf:pdf},
issn = {1439-0108},
journal = {Journal of Soils and Sediments},
keywords = {Detection method,NAPL,Non-aqueous phase-liquids (NAPL),Radon,Remediation,Soil contamination,Soil gas},
month = {dec},
number = {4},
pages = {217--222},
title = {{Using the soil gas radon as an indicator for ground contamination by non-aqueous phase-liquids}},
url = {http://link.springer.com/10.1007/BF02987728},
volume = {1},
year = {2001}
}
