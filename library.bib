keywords = {Athens,migration,urban transformation,vertical segregation},
month = {jul},
number = {5},
pages = {1--13},
title = {{Spatial segregation and migration in the city of Athens: Investigating the evolution of urban socio‐spatial immigrant structures}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/psp.2209},
volume = {25},
year = {2019}
}
