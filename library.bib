doi = {10.1016/j.geoderma.2005.07.016},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Vodnik et al. - 2006 - Small-scale spatial variation in soil CO2 concentration in a natural carbon dioxide spring and some related plant.pdf:pdf},
issn = {00167061},
journal = {Geoderma},
keywords = {Mofettes,Natural CO2 springs,Soil CO2 concentration,Spatial paterns},
