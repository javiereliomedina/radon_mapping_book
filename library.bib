isbn = {1876-6102},
issn = {18766102},
journal = {Energy Procedia},
keywords = {Accumulation chamber,CO2,Carbon isotopic signature,Gas flux measurement,Gas monitoring},
pages = {3526--3533},
publisher = {Elsevier},
title = {{First steps in coupling continuous carbon isotopic measurements with already proven subsurface gas monitoring methods above underground carbon dioxide storage sites}},
url = {http://dx.doi.org/10.1016/j.egypro.2011.02.280},
volume = {4},
