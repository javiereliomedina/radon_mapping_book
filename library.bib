issn = {0265931X},
journal = {Journal of Environmental Radioactivity},
keywords = {Indoor radon,Radon,Radon prone area,Radon survey,Uncertainty},
month = {jul},
pages = {6--10},
publisher = {Elsevier Ltd},
title = {{The uncertainty in the radon hazard classification of areas as a function of the number of measurements}},
volume = {173},
year = {2017}
