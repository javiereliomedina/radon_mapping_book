isbn = {3909168094},
issn = {03770273},
journal = {Journal of Volcanology and Geothermal Research},
keywords = {CO2 degassing,Somma-Vesuvius,Tectonics},
number = {1-4},
pages = {55--79},
title = {{Diffuse degassing of carbon dioxide at Somma-Vesuvius volcanic complex (Southern Italy) and its relation with regional tectonics}},
volume = {133},
year = {2004}
