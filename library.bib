issn = {1879-1700},
journal = {Journal of environmental radioactivity},
keywords = {Cave monitoring,Effective dose,Prevention,Radon exposures,Risk assessment,Subterranean workplaces},
pages = {19--29},
pmid = {25863322},
publisher = {Elsevier Ltd},
title = {{High radon levels in subterranean environments: monitoring and technical criteria to ensure human safety (case of Casta{\~{n}}ar cave, Spain).}},
volume = {145},
year = {2015}
}
