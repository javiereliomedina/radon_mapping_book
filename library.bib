isbn = {0091-6765 (Print)$\backslash$n0091-6765 (Linking)},
issn = {00916765},
journal = {Environmental Health Perspectives},
keywords = {Lung cancer,Radiation,Radon,Risk assessment,Uncertainty analysis},
number = {9},
pages = {1361--1366},
pmid = {16966089},
title = {{Lung cancer attributable to indoor radon exposure in France: Impact of the risk models and uncertainty analysis}},
volume = {114},
