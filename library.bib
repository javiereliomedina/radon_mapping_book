booktitle = {Agriculture Handbook Number 436},
doi = {10.1017/S0016756800045489},
edition = {Second Edi},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/USDA - 1999 - Soil Taxonomy. A Basic System of Soil Classification for Making and Interpreting Soil Surveys. Agriculture Handbook No. 43.pdf:pdf},
isbn = {Agriculture Handbook {\#} 436},
issn = {0016-7568},
pages = {754},
