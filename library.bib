@article{Font2003,
abstract = {The complexity generated by the existence of a great number of parameters and processes affecting the generation of radon in the source, its transport in the source medium, its entry into a dwelling and its accumulation in the different rooms of a dwelling has led to the development of partial models and experimental studies that are focused on a given aspect. However, in order to model radon levels and dynamics in real houses, it is necessary to take into account all the parameters and processes affecting radon levels. This is the objective of the dynamic RAGENA model of radon generation, entry and accumulation indoors. The model has been adapted to a Mediterranean climate house under dynamic conditions, and the indoor radon and soil radon dynamics have been compared to experimental results. It has been found (i) that the model gives a soil radon dynamics similar to that obtained experimentally, (ii) a remarkable model-experiment agreement indoors and (iii) that the indoor radon dynamics is given by a permanent radon entry from building materials and a dynamic removal through ventilation, which is driven by indoor-outdoor temperature differences and wind speed. {\textcopyright} 2002 Elsevier Science B.V. All rights reserved.},
author = {Font, Ll and Baixeras, C.},
doi = {10.1016/S0048-9697(02)00462-X},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Font, Baixeras - 2003 - The RAGENA dynamic model of radon generation, entry and accumulation indoors.pdf:pdf},
issn = {00489697},
journal = {Science of the Total Environment},
keywords = {Indoor radon,Radon dynamic modelling,Radon sources},
number = {1-3},
pages = {55--69},
title = {{The RAGENA dynamic model of radon generation, entry and accumulation indoors}},
volume = {307},
year = {2003}
}
