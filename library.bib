author = {W{\"{o}}lwer, Anna-Lena and Bre{\ss}lein, Martin and Burgard, Jan Pablo},
doi = {10.17713/ajs.v47i4.688},
file = {:C$\backslash$:/Users/FU53VP/OneDrive - Aalborg Universitet/Dokumenter/FLOW/01 Biblio/Modelling/688-ArticleText-2834-1-10-20180628.pdf:pdf},
issn = {1026-597X},
