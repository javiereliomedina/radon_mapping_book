doi = {10.3322/caac.20073},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Jemal et al. - 2010 - Cancer Statistics, 2010.pdf:pdf},
isbn = {1542-4863 (Electronic)$\backslash$r0007-9235 (Linking)},
