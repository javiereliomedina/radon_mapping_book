isbn = {0258-8900},
issn = {02588900},
journal = {Bulletin of Volcanology},
keywords = {Carbon dioxide,Diffuse degassing,Fumaroles,Hydrothermal gases,Monitoring,Steam condensation,Vesuvio},
number = {7},
pages = {642--651},
title = {{Diffuse CO2 degassing at Vesuvio, Italy}},
volume = {66},
year = {2004}
}
