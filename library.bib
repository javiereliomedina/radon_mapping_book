keywords = {Binary map,Binary random field,Data augmentation,Indicator kriging,Semivariogram,Spatial prediction},
number = {3},
pages = {299--314},
title = {{Bayesian prediction of clipped Gaussian random fields}},
volume = {34},
year = {2000}
}
