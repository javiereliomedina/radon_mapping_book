isbn = {9781424440740},
issn = {17505836},
journal = {International Journal of Greenhouse Gas Control},
keywords = {Accumulation chamber,CO2 storage,Diffuse CO2 soil flux,Statistical approaches},
month = {apr},
pages = {351--363},
title = {{A multi-statistical approach for estimating the total output of CO 2 from diffuse soil degassing by the accumulation chamber method}},
