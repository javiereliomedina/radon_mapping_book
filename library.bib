issn = {08832927},
journal = {Applied Geochemistry},
number = {1},
pages = {57--72},
title = {{U-series disequilibrium study on a gaseous discharge area (Solforata of Pomezia, Alban Hills, Italy): Implications for volcanic and geochemical risk}},
volume = {16},
year = {2001}
}
