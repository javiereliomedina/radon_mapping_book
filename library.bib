journal = {Chemosphere},
keywords = {Developing countries,Human exposure,Knapsack sprayer,Pesticide drift,Tracer},
number = {11},
pages = {1571--1577},
pmid = {21183200},
title = {{The weight method: A new screening method for estimating pesticide deposition from knapsack sprayers in developing countries}},
volume = {82},
year = {2011}
