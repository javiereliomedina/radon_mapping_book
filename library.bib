journal = {The Science of the total environment},
keywords = {Air Pollutants,Air Pollution,Atmosphere,Atmosphere: chemistry,Chemical,Geographic Information Systems,Indoor,Indoor: analysis,Indoor: statistics {\&} numerical data,Linear Models,Models,Northern Ireland,Permeability,Radiation Monitoring,Radiation Monitoring: methods,Radioactive,Radioactive: analysis,Radioactive: statistics {\&} numerical,Radiometry,Radon,Radon: analysis,Soil,Soil: chemistry},
month = {mar},
