isbn = {0921-030X},
issn = {0921030X},
journal = {Natural Hazards},
keywords = {Alban Hills quiescent volcano,Central Italy,Indoor-Rn hazard,Natural gas hazard},
number = {3},
pages = {257--287},
title = {{Natural gas hazard (CO 2, 222Rn) within a quiescent volcanic region and its relations with tectonics: The case of the Ciampino-Marino area, Alban Hills volcano, Italy}},
volume = {27},
year = {2002}
