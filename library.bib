journal = {Journal of Exposure Analysis and Environmental Epidemiology},
keywords = {Case-control studies,Dose-response relationship,Dosimetry,Epidemiologic methods,Lung neoplasms,Radon,Women's health},
number = {3},
pages = {197--203},
pmid = {12032816},
title = {{Residential radon exposure and lung cancer: Variation in risk estimates using alternative exposure scenarios}},
