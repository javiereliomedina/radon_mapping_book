keywords = {Air leakage,Indoor air,Modeling,Radon,Radon diffusion,Radon resistance},
number = {Supplement C},
pages = {343--350},
title = {{Development of a model for radon concentration in indoor air}},
volume = {416},
year = {2012}
}
