journal = {Atmospheric Environment},
keywords = {Emission factors,Greenhouse gases,Inventories,Municipal solid waste,Waste management},
number = {39},
pages = {7135--7142},
publisher = {Elsevier Ltd},
title = {{Methane emission estimation from landfills in Delhi: A comparative assessment of different methodologies}},
