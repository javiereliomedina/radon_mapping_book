issn = {13504487},
journal = {Radiation Measurements},
keywords = {Building materials,Cements,Gamma spectrometry,Natural radioactivity,Raw materials},
number = {8},
pages = {969--972},
publisher = {Elsevier Ltd},
title = {{Natural radioactivity and human exposure by raw materials and end product from cement industry used as building materials}},
url = {http://dx.doi.org/10.1016/j.radmeas.2010.06.023},
volume = {45},
year = {2010}
