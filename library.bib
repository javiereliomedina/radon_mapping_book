doi = {10.1016/j.rser.2013.12.002},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Nataly Echevarria Huaman, Xiu Jun - 2014 - Energy related CO2 emissions and the progress on CCS projects A review.pdf:pdf},
isbn = {1364-0321},
