journal = {International Journal of Greenhouse Gas Control},
pages = {294--302},
publisher = {Elsevier Ltd},
title = {{A parametric study of the transport of CO2 in the near-surface}},
url = {http://dx.doi.org/10.1016/j.ijggc.2012.04.007},
volume = {9},
year = {2012}
}
