issn = {0265931X},
journal = {Journal of Environmental Radioactivity},
keywords = {Gamma dose,Global Positioning System,Lifetime cancer risk,Radioactivity,Soil,Turkey},
month = {jan},
number = {1},
pages = {49--53},
pmid = {19038480},
publisher = {Elsevier Ltd},
title = {{Radionuclide concentrations in soil and lifetime cancer risk due to gamma radioactivity in Kirklareli, Turkey}},
