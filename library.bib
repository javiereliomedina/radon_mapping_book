isbn = {1750-5836},
issn = {17505836},
journal = {International Journal of Greenhouse Gas Control},
keywords = {Carbon capture and storage,Carbon isotopes,Geochemical tracing of CO2,Geological storage of CO2,Natural analogues,Noble gases},
number = {6},
pages = {1507--1516},
title = {{He and Ne as tracers of natural CO2 migration up a fault from a deep reservoir}},
