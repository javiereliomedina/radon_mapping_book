file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Magiera et al. - 2011 - Morphological and mineralogical forms of technogenic magnetic particles in industrial dusts.pdf:pdf},
isbn = {1352-2310},
issn = {13522310},
journal = {Atmospheric Environment},
keywords = {Industrial dusts,Iron mineralogy,Magnetic spherules,Technogenic magnetic particles},
number = {25},
pages = {4281--4290},
title = {{Morphological and mineralogical forms of technogenic magnetic particles in industrial dusts}},
