isbn = {9781450358163},
keywords = {Computational sustainability,Machine learning,Migration modeling},
pages = {1--8},
publisher = {ACM Press},
title = {{A Machine Learning Approach to Modeling Human Migration}},
url = {http://dl.acm.org/citation.cfm?doid=3209811.3209868},
year = {2018}
}
