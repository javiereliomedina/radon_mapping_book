keywords = {ARDL,Financial development,India,Rural regions,Rural-urban inequality,Urban regions},
month = {jan},
number = {2},
pages = {151--168},
title = {{Does financial development increase rural‐urban income inequality?}},
url = {https://www.emerald.com/insight/content/doi/10.1108/03068291311283616/full/html},
volume = {40},
year = {2013}
