keywords = {Electrical conductivity,GIS,Prediction model,Salinity index,Salinity model},
number = {3},
pages = {250--258},
publisher = {King Saud University {\&} Saudi Society of Agricultural Sciences},
title = {{Soil salinity modeling and mapping using remote sensing and GIS: The case of Wonji sugar cane irrigation farm, Ethiopia}},
url = {https://doi.org/10.1016/j.jssas.2016.05.003},
volume = {17},
year = {2018}
