journal = {Energy Conversion and Management},
keywords = {Carbon dioxide geological sequestration,Geochemical measurements,Surface leakage of CO2 and CH4},
month = {jul},
number = {12},
pages = {1921--1940},
title = {{Evaluation of leakage potential from a carbon dioxide EOR/sequestration project}},
volume = {44},
year = {2003}
}
