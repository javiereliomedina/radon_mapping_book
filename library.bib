file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Couwenberg et al. - 2011 - Assessing greenhouse gas emissions from peatlands using vegetation as a proxy.pdf:pdf},
