file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Barrio-Parra et al. - 2018 - Environmental risk assessment of cobalt and manganese from industrial sources in an estuarine system.pdf:pdf},
issn = {0269-4042},
journal = {Environmental Geochemistry and Health},
keywords = {Cobalt (Co),Estuary,Manganese (Mn),Mobility,Risk assessment},
month = {apr},
number = {2},
pages = {737--748},
publisher = {Springer Netherlands},
title = {{Environmental risk assessment of cobalt and manganese from industrial sources in an estuarine system}},
