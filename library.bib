journal = {Environmental Earth Sciences},
keywords = {Faults,Radon gas in soil,Radon mapping,Stochastic simulation,Uncertainty},
number = {3},
pages = {491--505},
title = {{A geostatistical approach for mapping and uncertainty assessment of geogenic radon gas in soil in an area of southern Italy}},
volume = {61},
year = {2010}
}
