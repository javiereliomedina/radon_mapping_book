isbn = {1876-6102},
issn = {18766102},
journal = {Energy Procedia},
keywords = {CO2,Claye-Souilly,Geochemical monitoring,Paris basin,Waste storage},
pages = {5438--5446},
publisher = {Elsevier},
title = {{CO2 flow baseline: Key factors of the geochemical monitoring program of future CO2 storage at Claye-Souilly (Paris basin)}},
url = {http://dx.doi.org/10.1016/j.egypro.2011.02.529},
volume = {4},
year = {2011}
}
