file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Garcia et al. - 2013 - The CO2-vadose project Numerical modeling to perform a geochemical monitoring methodology and baseline performanc.pdf:pdf},
issn = {17505836},
journal = {International Journal of Greenhouse Gas Control},
keywords = {CO2 leakage,Gas diffusion,Monitoring methodology,Natural CO2 dynamics,Numerical modeling,Vadose zone},
pages = {247--258},
publisher = {Elsevier Ltd},
title = {{The CO2-vadose project: Numerical modeling to perform a geochemical monitoring methodology and baseline performance assessment for various geochemical variables (gas flux, gas composition, stable isotopes and noble gases) in the carbonate vadose zone}},
url = {http://dx.doi.org/10.1016/j.ijggc.2013.01.029},
volume = {14},
