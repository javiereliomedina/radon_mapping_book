journal = {Journal of Geochemical Exploration},
keywords = {England,Geochemical behaviour,Model,Radon,Soils,Uranium,Water},
pages = {255--268},
title = {{Transfer of radon and parent nuclides 238U and 234U from soils of the Mendip Hills area, England, to the water phase}},
