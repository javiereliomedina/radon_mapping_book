@incollection{Rosario2006,
abstract = {Radon gas is the most important source of natural radioactivity. It is a radioactive noble gas that emanates from the ground. Radon concentrations in outdoor air are low, but in underground mines or inside houses radon concentrations can reach high levels. The biologic effects of radon are mostly due to its decay products. These are solid heavy metal isotopes that are themselves radioactive. They can be deposited on the bronchial epithelium, where they emit ionizing radiation in the form of $\alpha$-particles. Alpha radiation can effectively cause permanent damage to the DNA and can eventually lead to cancer. Epidemiological studies have given conclusive evidence that radon is carcinogenic to humans and can cause lung cancer. The first studies were performed in underground miners exposed to high levels of radon. Later studies performed in the general population showed that radon can cause lung cancer even at the lower concentrations found in houses. This indoor radon exposure is currently estimated to contribute to 5–10{\%} of lung cancer deaths.},
author = {Rosario, A.S. and Wichmann, H.-E.},
booktitle = {Encyclopedia of Respiratory Medicine},
doi = {10.1016/B0-12-370879-6/00135-6},
isbn = {9780123708793},
keywords = {Attributable risk,DNA damage,Environmental pollutants,Epidemiology,Ionizing radiation,Lung cancer,Occupational diseases,Radon,Smoking,Tumors},
pages = {120--125},
