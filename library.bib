file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hrachowitz et al. - 2010 - Thermal regimes in a large upland salmon river A simple model to identify the influence of landscape controls.pdf:pdf},
isbn = {0885-6087},
issn = {08856087},
journal = {Hydrological Processes},
keywords = {Climate change,Spatial temperature distribution,Stream temperature,Temperature controls},
number = {23},
pages = {3374--3391},
title = {{Thermal regimes in a large upland salmon river: A simple model to identify the influence of landscape controls and climate change on maximum temperatures}},
volume = {24},
year = {2010}
}
