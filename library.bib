doi = {10.1016/j.scitotenv.2011.09.088},
file = {:C$\backslash$:/Users/FU53VP/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Statham - 2012 - Nutrients in estuaries - An overview and the potential impacts of climate change.pdf:pdf},
isbn = {0048-9697},
issn = {00489697},
journal = {Science of the Total Environment},
keywords = {Biogeochemistry,Climate-change,Estuaries,Eutrophication,Nutrients},
pages = {213--227},
pmid = {22119025},
publisher = {Elsevier B.V.},
title = {{Nutrients in estuaries - An overview and the potential impacts of climate change}},
url = {http://dx.doi.org/10.1016/j.scitotenv.2011.09.088},
volume = {434},
year = {2012}
}
